local kickMessage = "Join Jotunnheim Server And Roblox Group for Jotunnheim Access! Discord Server Has Been Copied To Clipboard"

local function checkPlayerGroupMembership(player)
    if not player:IsInGroup(groupId) then   
        setclipboard(discordInvite)
        game.Players.LocalPlayer:Kick(kickMessage)
    end
end
    checkPlayerGroupMembership(game.Players.LocalPlayer)
    

