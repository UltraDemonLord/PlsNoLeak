addCommand("platform",{},function()
    Loops.platform = true
    repeat game:GetService("RunService").RenderStepped:Wait() pcall(function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Head then
            if not game.Players.LocalPlayer.Character:FindFirstChild("PLATFORM###14") then
                local fakepart=Instance.new("Part",game.Players.LocalPlayer.Character)fakepart.Name="PLATFORM###14"fakepart.Size=Vector3.new(10,1,10)fakepart.Anchored = true;fakepart.CanCollide = true;fakepart.Color = game.Players.LocalPlayer.Character.Torso.Color;if game.Players.LocalPlayer.Name == "ii_SaucyyRon" then fakepart.Color=Color3.new(1,0.5,0) end;fakepart.TopSurface="Smooth"fakepart.BottomSurface="Smooth"fakepart.Material = "SmoothPlastic"
            else
                local fakepart=game.Players.LocalPlayer.Character:FindFirstChild("PLATFORM###14")
                fakepart.Color=game.Players.LocalPlayer.Character.Torso.Color
		if game.Players.LocalPlayer.Name == "ii_SaucyyRon" then fakepart.Color=Color3.new(1,0.5,0) end
                fakepart.CFrame=CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.X,0.199999332,game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Z) * CFrame.Angles(0,math.rad(game.Players.LocalPlayer.Character.HumanoidRootPart.Orientation.Y),0) 
            end
        end
    end)
    until not Loops.platform
end)

addCommand("removeplatform",{},function()
    Loops.platform = false
    if game.Players.LocalPlayer.Character:FindFirstChild("PLATFORM###14") then
    game.Players.LocalPlayer.Character:FindFirstChild("PLATFORM###14"):Destroy()
    end
end)

addCommand("unplatform",{},function()
    runCommand(prefix.."removeplatform",{})
end)

