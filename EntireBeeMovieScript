local response = HttpService:GetAsync(url)
local returned = HttpService:JSONDecode(response)
if not returned then
    chat(botname.. "Catalog Audio API didn't return anything.")
    return
end
local music = returned.Items[1]
if music and music.AssetId then
    tchat("music "..tostring(music.AssetId))
else
    chat(botname.. "No Audio found for that search")
end
end)]]

