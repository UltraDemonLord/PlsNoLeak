                local con69;
                con69 = Folder.ChildAdded:Connect(function(c)
                if c.Name == "Part" and c.Size == Vector3.new(x,y,z) then 
                pcall(function() con69:Disconnect() end)
                part = c;
                end
                end)

                

                
                
                repeat task.wait() until part
                part.Name = "newpart"
                if c then
                    part.Color = c;
                    colorAPI.color(part, c)
                end
