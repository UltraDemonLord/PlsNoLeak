    tchat(".visualizer 84 143 111 0 7 10 30")
    task.wait(2)
    tchat("music 6717030010")
    task.wait(7)
    chat(botname.. 'Partys over')
    task.wait(3)
    tchat(".unvisualizer")
    tchat("music ")
    tchat("fix")
end)

