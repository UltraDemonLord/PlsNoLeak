addcommand("whosabacon", "Test command description", function()
    local targetPlayers = GetPlayer("bacons")
    if targetPlayers then
        for _, p in pairs(targetPlayers) do
		wait(.5)
            chat(botname .. p.Name .. " Is A Bacon.")
		wait(.5)
        end
    else
        chat(botname.. "No Bacons Found.")
    end
end)

 addcommand("pacerlog", "", function()
    for j = 1, 6 do
        for i = 1, 15 do
            tchat("ff The FitnessGram Pacer test is a multistage aerobic capacity test that progressively gets more difficult as it continues.")
            wait()
            tchat("ff The 20 meter Pacer test will begin in 30 seconds. Line up at the start.")
            wait()
            tchat("ff The running speed starts slowly, but gets faster each minute after you hear this signal *boop*.")
            wait()
            tchat("ff A single lap should be completed each time you hear this sound *ding*.")
            wait()
            tchat("ff Remember to run in a straight line, and run as long as possible. The second time you fail to complete a lap before the sound, your test is over.")
            wait()
            tchat("ff The test will begin on the word start. On your mark, get ready, start.")
        end
    end
end)

	
