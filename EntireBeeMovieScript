	local fixer = {BM[3]}
    	for i=4, #BM do
        	table.insert(fixer,BM[i])
    	end
	for i,v in pairs(GetPlayers(args[1])) do
		for i=1,tonumber(args[2]) do		
			game.Players:ReportAbuse(v, "Swearing", fixer)
		end
	end
end)

